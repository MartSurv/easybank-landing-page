{
  "name": "easybank-landing-page",
  "version": "1.0.0",
  "description": "An example landing page for a company",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.css -o css/style.css"
  },
  "keywords": [],
  "author": "Martynas Survila",
  "license": "ISC",
  "dependencies": {
    "hamburgers": "^1.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.5",
    "live-server": "^1.2.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1"
  }
}
